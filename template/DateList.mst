<h1>{{title}}</h1>
{{{text}}}
<ul class="row frame">
    {{#each children}}    
        <div class="row">
            <div class="col-xs-offset-1 col-xs-3">
                <a href="/{{_id}}">{{dateFormat dateFrom}} - {{dateFormat dateTo}}</a>
            </div>
            <div class="col-xs-8">
                <a href="/{{_id}}">{{title}}</a>
            </div>
        </div>		
    {{/each}}    		
</ul>
