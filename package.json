{
  "name": "admin5",
  "version": "0.1.0",
  "description": "CMS Admin based on OpenUI5",
  "private": true,
  "scripts": {
    "start": "ui5 serve",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "watch": "npm run karma",
    "test": "npm run lint && rimraf coverage && npm run karma-ci",
    "build": "rimraf dist && ui5 build --a",
    "server": "node server.js"
  },
  "devDependencies": {
    "@ui5/cli": "^2.5.0",
    "eslint": "^4.19.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-ui5": "^2.2.0",
    "rimraf": "^3.0.2",
    "ui5-middleware-simpleproxy": "^0.5.1"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-simpleproxy"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "express-http-proxy": "^1.6.2",
    "morgan": "^1.10.0",
    "mustache-express": "^1.3.0",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-find": "^7.2.2"
  }
}
